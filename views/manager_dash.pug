extends layout
block content
    head
        title Dashboard
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css")
    body
        .container
        nav.navbar.navbar-success.bg-success
            span.navbar-brand.text-white Manager Dashboard 
        .card
            .card-body
                | Welcome Jacob!
            .col-md-3
            .list-group
                a.list-group-item.list-group-item-success(href="/users") Registered Users
                a.list-group-item.list-group-item-success(href="/register_furniture") Register Furniture Stock
                a.list-group-item.list-group-item-success(href="/register_wood") Register Wood Stock
                a.list-group-item.list-group-item-success(href="/furniture_list") Furniture Stock List
                a.list-group-item.list-group-item-success(href="/wood_list") Wood Stock List
                a.list-group-item.list-group-item-success(href="/furnituresales") Furniture Sales
                a.list-group-item.list-group-item-success(href="/woodsales") Wood Sales
                a.list-group-item.list-group-item-danger(href="/logout") Logout
            .col-md-9
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js" integrity="sha384-FKyoEForCGlyvwx9Hj09JcYn3nv7wiPVlz7YYwJrWVcXK/BmnVDxM+D2scQbITxI" crossorigin="anonymous")       



